# To ignore OS temporary files use global .gitignore
# https://help.github.com/articles/ignoring-files/#create-a-global-gitignore

# Ignore local/sensitive information
settings.local.php
services.local.yml

# Ignore directories generated by composer/etc.
/drush/contrib/
/vendor/
/node_modules/
/docroot/core/
/docroot/modules/contrib/
/docroot/themes/contrib/
/docroot/profiles/govcms/
/docroot/libraries/

# Ignore default text files and templates.
/CHANGELOG.txt
/COPYRIGHT.txt
/INSTALL*.txt
/LICENSE.txt
/MAINTAINERS.txt
/UPGRADE.txt
/README.txt
sites/README.txt
sites/all/modules/README.txt
sites/all/themes/README.txt
default.services.yml
default.settings.php
example.sites.php

# Ignore Drupal's file directory
/docroot/sites/*/files
/docroot/sites/*/private

# Dev related files.
/docroot/sites/simpletest
/.idea/
.vagrant
*.retry
.data
screenshots
.vault_pass
/acquiacli.yml
